generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Command {
  ID               Int         @id @default(autoincrement())
  GameTitle        String
  CharacterName    String
  Input      String
  SkillName        String      @unique
  CommandImagePath Json
  HitParts         HitPartEnum

  @@unique( fields:[GameTitle,CharacterName,Input],name: "CommandPerCharacter")
  @@unique( fields:[GameTitle,CharacterName,SkillName],name: "SkillPerCharacter")

}

enum HitPartEnum {
  High
  Middle
  Low
}
